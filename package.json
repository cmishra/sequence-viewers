{
  "name": "@ninjha01/nitro-ui",
  "private": false,
  "version": "0.0.77",
  "scripts": {
    "build": "pnpm lint && vite build && pnpm build-css",
    "build-css": "npx tailwindcss -o ./nitro.css --minify && mv ./nitro.css ./dist/nitro.css",
    "dev": "pnpm storybook dev -p 6006",
    "build-storybook": "pnpm build && storybook build",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --check --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepublishOnly": "pnpm lint && pnpm format && pnpm build",
    "watch": "nodemon --watch src --ext js,jsx,ts,tsx,json,css,scss,html --exec pnpm build"
  },
  "dependencies": {
    "3dmol": "^1.8.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@types/d3": "^7.4.0",
    "@types/d3-hierarchy": "^3.1.3",
    "class-variance-authority": "^0.2.4",
    "clsx": "^1.2.1",
    "d3": "^7.8.5",
    "d3-hierarchy": "^3.1.2",
    "daisyui": "^2.52.0",
    "jotai": "^1.13.1",
    "molstar": "^3.39.0",
    "react-hook-form": "^7.46.1",
    "react-markdown": "^8.0.7",
    "react-render-if-visible": "^2.1.1",
    "reactflow": "^11.8.3",
    "remark-gfm": "^3.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@rollup/plugin-alias": "^4.0.4",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-mdx-gfm": "^7.4.1",
    "@storybook/blocks": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-vite": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^8.3.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.13",
    "json": "^11.0.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.1",
    "storybook-tailwind-dark-mode": "^1.0.22",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^1.7.3"
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/nitro-ui.umd.js",
  "module": "./dist/nitro-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nitro-ui.es.js",
      "require": "./dist/nitro-ui.umd.js"
    }
  },
  "description": "collection of ui components",
  "keywords": [
    "ui",
    "components"
  ],
  "author": "Nishant Jha"
}
