{
  "name": "@ninjha01/nitro-ui",
  "private": false,
  "version": "0.0.74",
  "scripts": {
    "build": "tsc && vite build && yarn build-css",
    "build-css": "npx tailwindcss -o ./nitro.css --minify && mv ./nitro.css ./dist/nitro.css",
    "dev": "yarn storybook dev -p 6006",
    "build-storybook": "yarn build && storybook build",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --check --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepublishOnly": "yarn lint && yarn format && yarn build",
    "watch": "nodemon --watch src --ext js,jsx,ts,tsx,json,css,scss,html --exec yarn build"
  },
  "dependencies": {
    "3dmol": "^1.8.0",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@types/d3": "^7.4.0",
    "class-variance-authority": "^0.2.4",
    "clsx": "^1.2.1",
    "d3": "^7.8.2",
    "d3-hierarchy": "^3.1.2",
    "daisyui": "^2.51.5",
    "jotai": "^1.11.1",
    "molstar": "^3.26.0",
    "react-hook-form": "^7.43.9",
    "react-markdown": "^8.0.3",
    "react-render-if-visible": "^2.1.1",
    "reactflow": "^11.3.3",
    "remark-gfm": "^3.0.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@rollup/plugin-alias": "^4.0.2",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-vite": "^7.0.20",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^0.6.12",
    "json": "^11.0.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.19",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.20",
    "storybook-tailwind-dark-mode": "^1.0.22",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^1.6.6"
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/nitro-ui.umd.js",
  "module": "./dist/nitro-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nitro-ui.es.js",
      "require": "./dist/nitro-ui.umd.js"
    }
  },
  "description": "collection of ui components",
  "keywords": [
    "ui",
    "components"
  ],
  "author": "Nishant Jha"
}
